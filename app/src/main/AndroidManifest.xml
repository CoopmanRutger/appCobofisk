<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rutgercoopman.howest.projectapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".screens.Deliverys" />
        <activity android:name=".screens.Storedetails" />
        <activity android:name=".screens.Stores" />
        <activity android:name=".screens.Employees" />
        <activity
            android:name=".screens.Homescreen"
            android:label="@string/title_activity_homescreen" />
        <activity android:name=".screens.Addemployee" />
        <activity
            android:name=".screens.Login_screen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>